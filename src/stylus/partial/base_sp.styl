@import '../libs/easing'

html, body
  position relative
  min-width 320px
  font-family "ヒラギノ角ゴ ProN W3", "Hiragino Kaku Gothic ProN", "游ゴシック", YuGothic, "メイリオ", Meiryo, sans-serif
  font-size 10px
  line-height 1
  -webkit-font-smoothing antialiased

a
  text-decoration inherit
  color inherit
  -webkit-tap-highlight-color rgba(0,0,0,0)

img
  display block
  width 100%
